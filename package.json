{
  "name": "node-ts-express-docker-starter",
  "version": "1.0.0",
  "description": "Pre-configured Node.js project with TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:docker": "docker build -t $(npx jsonfield package.json name) .",
    "start": "docker run -p 8080:8080 -t $(npx jsonfield package.json name)",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "dev": "nodemon"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.41.0",
    "jsonfield": "^1.0.3",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/node": "^18.15.11",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "typescript": "^5.0.2"
  }
}
